{
  "version": 2,
  "builds": [
    { "src": "api/health.js", "use": "@vercel/node" },
    { "src": "api/login.js", "use": "@vercel/node" },
    { "src": "api/register.js", "use": "@vercel/node" },
    { "src": "*.html", "use": "@vercel/static" },
    { "src": "auth.js", "use": "@vercel/static" },
    { "src": "script.js", "use": "@vercel/static" },
    { "src": "style.css", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/health", "dest": "/api/health.js" },
    { "src": "/api/login", "dest": "/api/login.js" },
    { "src": "/api/register", "dest": "/api/register.js" },
    { "src": "/", "dest": "/login.html" },
    { "src": "/login.html", "dest": "/login.html" },
    { "src": "/register.html", "dest": "/register.html" },
    { "src": "/index.html", "dest": "/index.html" },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/$1" }
  ]
}
