{
  "version": 2,
  "builds": [
    { "src": "api/login.js", "use": "@vercel/node" },
    { "src": "api/register.js", "use": "@vercel/node" },
    { "src": "api/[[...path]].js", "use": "@vercel/node" },
    { "src": "api/index.js", "use": "@vercel/node" },
    { "src": "*.html", "use": "@vercel/static" },
    { "src": "auth.js", "use": "@vercel/static" },
    { "src": "script.js", "use": "@vercel/static" },
    { "src": "style.css", "use": "@vercel/static" }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/", "dest": "/index.html" },
    { "src": "/(.*)", "dest": "/$1" }
  ]
}
